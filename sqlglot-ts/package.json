{
  "name": "sqlglot-ts",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dialects/postgres": {
      "types": "./dist/dialects/postgres.d.ts",
      "import": "./dist/dialects/postgres.js"
    },
    "./dialects/bigquery": {
      "types": "./dist/dialects/bigquery.d.ts",
      "import": "./dist/dialects/bigquery.js"
    },
    "./dialects/duckdb": {
      "types": "./dist/dialects/duckdb.d.ts",
      "import": "./dist/dialects/duckdb.js"
    },
    "./dialects/mysql": {
      "types": "./dist/dialects/mysql.d.ts",
      "import": "./dist/dialects/mysql.js"
    },
    "./dialects/redshift": {
      "types": "./dist/dialects/redshift.d.ts",
      "import": "./dist/dialects/redshift.js"
    },
    "./dialects/clickhouse": {
      "types": "./dist/dialects/clickhouse.d.ts",
      "import": "./dist/dialects/clickhouse.js"
    },
    "./dialects/databricks": {
      "types": "./dist/dialects/databricks.d.ts",
      "import": "./dist/dialects/databricks.js"
    },
    "./dialects/sqlite": {
      "types": "./dist/dialects/sqlite.d.ts",
      "import": "./dist/dialects/sqlite.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "tsdown": "^0.9.0"
  }
}
